{
  "name": "near-account-lookup",
  "displayName": "NEAR Account Lookup",
  "description": "Detects NEAR account IDs, shows balance/activity on hover, and links to explorer.",
  "version": "0.1.1",
  "publisher": "gemini-on-near",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mastrophot/near-account-lookup-vscode.git"
  },
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "configuration": {
      "title": "NEAR Account Lookup",
      "properties": {
        "nearAccountLookup.network": {
          "type": "string",
          "default": "mainnet",
          "enum": ["mainnet", "testnet"],
          "description": "NEAR network to use for lookups."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@types/vscode": "^1.75.0",
    "typescript": "^4.9.5"
  }
}
